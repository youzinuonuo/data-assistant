{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Streamlit: Run App",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "args": [
                "run",
                "app.py"
            ],
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
    ]
}
